{
  "name": "atomicagent",
  "version": "0.4.6-next.0",
  "description": "Atomic Swap Agent",
  "main": "src/index.js",
  "bin": {
    "atomicagent": "./bin/atomicagent",
    "atomicagent-api": "./bin/atomicagent-api",
    "atomicagent-wallet": "./bin/atomicagent-wallet",
    "atomicagent-worker": "./bin/atomicagent-worker"
  },
  "scripts": {
    "api": "PROCESS_TYPE=api nodemon src/index.js",
    "worker": "PROCESS_TYPE=worker nodemon src/index.js",
    "migrate": "PROCESS_TYPE=migrate node src/index.js",
    "api-service": "MIGRATE_KEEP_ALIVE=true node src/index.js",
    "migrate-local": "CONFIG_PATH=env/local/config.local.toml PROCESS_TYPE=migrate node src/index.js",
    "docker:build-local": "docker build -f api.dockerfile --build-arg ENV_ALIAS=local -t atomicagent-local . --no-cache",
    "docker:run-local": "docker run --rm --name atomicagent -d -it atomicagent-local:latest",
    "docker:stop-local": "docker stop atomicagent",
    "docker:log-local": "docker container logs -t atomicagent",
    "docker:start": "docker-compose -f test/docker/docker-compose.yml up -d --force-recreate --renew-anon-volumes",
    "docker:stop": "docker-compose -f test/docker/docker-compose.yml down",
    "lint": "standard",
    "flint": "standard --fix",
    "precommit-message-lint": "printf '\nPre-commit checks (linting)...\n' && exit 0",
    "test": "DEBUG=liquality:agent* NODE_ENV=test CONFIG_PATH=test.config.toml mocha --exit --bail ./test/*.test.js",
    "test:unit": "NODE_ENV=test mocha test/unit/**"
  },
  "pre-commit": [
    "precommit-message-lint",
    "lint"
  ],
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "author": "Liquality <info@liquality.io>",
  "repository": {
    "type": "git",
    "url": "git@github.com:liquality/agent.git"
  },
  "license": "MIT",
  "dependencies": {
    "@liquality/bitcoin-esplora-batch-api-provider": "1.1.8",
    "@liquality/bitcoin-esplora-swap-find-provider": "1.1.8",
    "@liquality/bitcoin-fee-api-provider": "1.1.8",
    "@liquality/bitcoin-js-wallet-provider": "1.1.8",
    "@liquality/bitcoin-networks": "1.1.8",
    "@liquality/bitcoin-node-wallet-provider": "1.1.8",
    "@liquality/bitcoin-rpc-fee-provider": "1.1.8",
    "@liquality/bitcoin-rpc-provider": "1.1.8",
    "@liquality/bitcoin-swap-provider": "1.1.8",
    "@liquality/client": "1.4.3",
    "@liquality/client-factory": "1.1.8",
    "@liquality/crypto": "1.1.8",
    "@liquality/cryptoassets": "^1.2.6",
    "@liquality/ethereum-erc20-provider": "1.1.8",
    "@liquality/ethereum-erc20-scraper-swap-find-provider": "1.1.8",
    "@liquality/ethereum-erc20-swap-provider": "1.1.8",
    "@liquality/ethereum-gas-now-fee-provider": "1.1.8",
    "@liquality/ethereum-js-wallet-provider": "1.1.8",
    "@liquality/ethereum-networks": "1.4.3",
    "@liquality/ethereum-rpc-fee-provider": "1.1.8",
    "@liquality/ethereum-rpc-provider": "1.1.8",
    "@liquality/ethereum-scraper-swap-find-provider": "1.1.8",
    "@liquality/ethereum-swap-provider": "1.4.3",
    "@liquality/near-js-wallet-provider": "1.6.0",
    "@liquality/near-networks": "1.1.8",
    "@liquality/near-rpc-provider": "1.6.0",
    "@liquality/near-swap-find-provider": "1.1.8",
